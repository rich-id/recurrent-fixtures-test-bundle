{
    "name": "richcongress/recurrent-fixtures-test-bundle",
    "description": "A magic bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "RichCongress",
            "homepage": "https://www.richcongress.com"
        }
    ],
    "support": {
        "email": "contact@richcongress.com",
        "source": "https://github.com/richcongress/recurrent-fixtures-test-bundle",
        "docs": "https://github.com/richcongress/recurrent-fixtures-test-bundle/blob/master/README.md",
        "issues": "https://github.com/richcongress/recurrent-fixtures-test-bundle/issues"
    },
    "require": {
        "php": ">=7.3",
        "doctrine/doctrine-fixtures-bundle": "3.5.x-dev",
        "richcongress/bundle-toolbox": "*",
        "richcongress/web-test-bundle": "dev-master"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "infection/infection": "^0.16.4",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "RichCongress\\RecurrentFixturesTestBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RichCongress\\RecurrentFixturesTestBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "path",
            "url": "/home/nover/Bundles/WEB-TEST",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/home/nover/Bundles/TEST-FRAMEWORK",
            "options": {
                "symlink": true
            }
        }
    ]
}
